<!--================Blog section Start =================-->
<section class="section-padding bg-gray">
  <div class="container">
    <div class="section-intro text-center pb-90px">
      <img class="section-intro-img" src="/default/img/home/section-icon.png" alt="">
      <h1 class="home-blogs">CollegeTime Blogs</h1>
    </div>
    <div class="row">
      <% for (var i = 0; i < 3; i++) {%>
        <div class="blog">
                <article class="cardt" style="height:505px;">
                  <div class="card-img-top">
                    <img class="card-img rounded-0" src="<%=posts[i].file%>" height=230px alt="">
                </div>

                  <div class="card-body cont">
                      <a class="card-title cardtitle" style="text-decoration : none" href="/blogs/<%=posts[i].slug%>">
                          <h3><%=posts[i].title.substring(0,60)%></h3>
                      </a>
                      <div class"cardtext">
                      <p class="card-text"><%-posts[i].description.substring(0,100)%></p>
                    </div>
                  </div>
                    <div class="daefooter">
                      <ul id="daelist">
                        <li><p id="dae"><%=posts[i].creationDate%></p></li>
                      </ul>
                    </div>
                    <div class="daefooter2">
                      <ul id="daelist">
                        <li><p id="daeaut" style="text-align:right;"><%=posts[i].author?.author%></p></li>
                      </ul>
                    </div>

                </article>
              </div>
                  <%};%>
                                      </div>

    </div>
    </section>

<section class="section-margin">
  <div class="contain">
    <div class="row">
      <div class="col-md-12">
        <div  id="subscribe" class="search-wrapper">
          <h2 style="color:#32325d;">Subscribe To Our Newsletter</h2>
          <p class="responsive">Be a part of our ever growing community.<a href="https://instagram.com/collegetime.co"> Follow us on Instagram</a> </p>
          <form class="form">
              <input class="email" id="email" type="email" name="email" class="form-control" placeholder="Enter Your Email address ">
              <button class="newslettersubmit" id="create-newsletter-button" type="submit">Subscribe</button>
          </form>
          <p class="responsive2"><a href="https://instagram.com/collegetime.co"> Follow us on Instagram</a> </p>
          <p id="newssuccess"></p>
        </div>

      </div>
    </div>
  </div>
</section>
<!--================Blog section End =================-->





