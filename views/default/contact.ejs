      <script src="/default/vendors/jquery/jquery-3.2.1.min.js"></script>
      <script src="/default/vendors/bootstrap/bootstrap.bundle.min.js"></script>
      <script src="/default/vendors/owl-carousel/owl.carousel.min.js"></script>
      <script src="/default/vendors/nice-select/jquery.nice-select.min.js"></script>
      <script src="/default/js/jquery.ajaxchimp.min.js"></script>
      <script src="/default/js/mail-script.js"></script>
      <script src="/default/js/main.js"></script>
      <script>
      $("#create-contact-button").on('click', function (e) {
          e.preventDefault();
          // console.log("this is check point 1 ");

          var data = $("#message").val();
          var name = $("#name").val();
          var mail = $("#mail").val();
          var subject = $("#subject").val();
          // alert(data); //just to debug

        $.ajax({
              url: '/admin/contact/create',
             type: 'POST',
              data: {message: data,name:name,mail:mail,subject:subject},
              success: function (response) {
                document.getElementById("contactsuccess").innerHTML="Success";
                  console.log(response);
      //category list is the class of tbody in admin/category/index

              }
          });
            $("#message").val('');
            $("#name").val('');
            $("#mail").val('');
            $("#subject").val('');
        });
      </script>
      <script>

        $("#create-newsletter-button").on('click', function (e) {
            e.preventDefault();
            // console.log("this is check point 1 ");

            var data = $("#email").val();
            // alert(data); //just to debug

          $.ajax({
                url: '/admin/newsletter/create',
               type: 'POST',
                data: {mail: data},
                success: function (response) {
                  document.getElementById("newssuccess").innerHTML="Successful Registration";
                    console.log(response);
      //category list is the class of tbody in admin/category/index

                }
            });
              $("#email").val('');
          });
      </script>

	<!-- ================ contact section end ================= -->






